---
import Earth from "@/components/Earth.astro"
import SectionContainer from "@/components/SectionContainer.astro"

interface Props {
  delay: string
  class?: string
}

const { delay, class: className } = Astro.props
---

<SectionContainer
  delay={delay}
  class:list={[className, "flex items-center justify-center"]}
>
  <Earth />
</SectionContainer>
